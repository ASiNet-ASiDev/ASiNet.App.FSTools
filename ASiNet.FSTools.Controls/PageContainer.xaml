<Grid x:Class="ASiNet.FSTools.Controls.PageContainer"
      xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
      xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
      xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
      xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
      xmlns:local="clr-namespace:ASiNet.FSTools.Controls"
      mc:Ignorable="d" 
      d:DesignHeight="450" d:DesignWidth="800" Margin="2, 2, 2, 2" Background="White"
      x:Name="Root">
    <TextBlock x:Name="TitleText" Text="Container" HorizontalAlignment="Center" VerticalAlignment="Center" Panel.ZIndex="0"/>
    <Grid.RowDefinitions>
        <RowDefinition Height="*"/>
    </Grid.RowDefinitions>
    <Grid.ColumnDefinitions>
        <ColumnDefinition Width="*"/>
    </Grid.ColumnDefinitions>
</Grid>
